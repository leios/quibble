void simple_print(){
    printf("printing from kernel...\n");
}

__verse check(x = 7; y = 8;){
    simple_print();
}

__verse check2(q = a*b*c; z = q*r;){
    // This comment shouldn't break anything
    q += z;
}

void is_this_found(){};

__stanza stanza_check(a, b, c | r = a*b;){
    @VCALL check2();
    __split_stanza();
    @VCALL check();
}

__poem poem_check(a, b | c = 5;){
    @SCALL stanza_check(a, b, c){
        simple_print();
    }
}
